{
    "version": 2,
    "builds": [
      { "src": "server/index.js", "use": "@vercel/node" },
      { "src": "web/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/index.js" },
      { "src": "/(.*)", "dest": "web/build/$1" }
    ]
  }
  